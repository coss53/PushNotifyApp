{
  "name": "pushnotifyapp",
  "license": "0BSD",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "start": "expo start",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eas-build-pre-install": "node ./scripts/createGoogleServicesFile.js",
    "eas-build-pre-install-windows": "powershell -Command \"[System.Text.Encoding]::UTF8.GetString([System.Convert]::FromBase64String($env:GOOGLE_SERVICES_BASE64)) | Out-File -Encoding UTF8 android/app/google-services.json\"",
    "eas-build-pre-install-linux": "echo $GOOGLE_SERVICES_BASE64 | base64 -d > android/app/google-services.json"
  },
  "dependencies": {
    "expo": "~53.0.20",
    "expo-constants": "~17.1.7",
    "expo-dev-client": "~5.2.4",
    "expo-notifications": "~0.31.4",
    "expo-status-bar": "~2.2.3",
    "react": "19.0.0",
    "react-native": "0.79.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0"
  },
  "private": true
}
